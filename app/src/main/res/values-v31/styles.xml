<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="TXTS">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">61dp</item>
        <item name="android:gravity">center|left</item>
        <item name="android:textColor">@android:color/system_accent2_700</item>
    </style>

    <style name="TXTSS" tools:ignore="DuplicateDefinition">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">61dp</item>
        <item name="android:gravity">center|left</item>
        <item name="android:textColor">@android:color/system_accent2_700</item>
    </style>


    <style name="txtStyle" parent="Widget.MaterialComponents.TextView" tools:ignore="DuplicateDefinition">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:gravity">center|left</item>
        <item name="android:shadowColor">#737171</item>
        <item name="android:textColor">#1D2325</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textStyle">bold</item>
    </style>
</resources>